    "@types/react-dom": "18.2.4",
    "@yudiel/react-qr-scanner": "^2.1.0",
    "autoprefixer": "10.4.14",
    "axios": "^1.7.9",
    "concurrently": "^8.0.1",
    "daisyui": "^4.12.14",
    "eslint": "8.40.0",
